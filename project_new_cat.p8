function ienemies()
  enemies = {}
end

function enemyspawn(type, locationx, locationy, flag)
  if type == "redclone" then
    sprite = 52
  elseif type == "knight" then
    sprite = 32
  else
    sprite = 0
  end

  sprite = 52
  add(
    enemies, {
      flag = flag,
      type = type,
      sp = sprite,
      x = locationx,
      y = locationy,
      start_x = locationx,
      start_y = locationy,
      w = 8,
      h = 8,
      flp = false,
      sx = 0,
      dx = 0,
      dy = 0,
      max_dx = 2,
      max_dy = 3,
      acc = 0.5,
      boost = 4,
      anim = 0,
      running = false,
      jumping = false,
      falling = false,
      sliding = false,
      landed = false,
      done = 0
    }
  )
end

function update_enemies()
  for e in all(enemies) do
    e.dy += gravity
    e.dx *= friction

    if e.dy > 0 then
      e.falling = true
      e.landed = false
      e.jumping = false

      e.dy = limit_speed(e.dy, e.max_dy)

      if collide_map(e, "down", 0) then
        e.landed = true
        e.falling = false
        e.dy = 0
        e.y -= (e.y + e.h + 1) % 8 - 1
        e.wallsliding = false
      end
    elseif e.dy < 0 then
      e.jumping = true
      if collide_map(e, "up", 1) then
        e.dy = 0
      end
    end
    e.x += e.dx
    e.y += e.dy

    --bad code
    if e.type == "redclone" then
      -- Pacing back and forth logic
      if e.dx == 0 then
        e.dx = e.max_dx -- Start moving to the right
        e.flp = false
      elseif e.x >= e.start_x + 10 then
        e.dx = -e.max_dx -- Change direction when reaching the right limit
        e.flp = true
      elseif e.x <= e.start_x then
        e.dx = e.max_dx -- Change direction when reaching the left limit
        e.flp = false
      end
      --animating for redclone
      e.anim += 1
      if e.anim > 3 then
        e.anim = 0
      end
      if e.anim == 0 or e.anime == 1 then
        e.sp = 51
      else
        e.sp = 52
      end
    end
  end
end

function draw_enemies()
  for e in all(enemies) do
    spr(e.sp, e.x, e.y, 1, 1, e.flp)
  end
end

