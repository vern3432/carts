function ubullets()
  player.ninjastarstatus += 1
  for e in all(buls) do
    e.x += e.spd
    e.distance += e.spd

    --animate sprite
    if e.distance % 2 == 0 then
      e.sprite = 13
    else
      e.sprite = 14
    end
    --animate end of distance

    if e.distance == 18 or e.distance < -18 then
      e.sprite = 15
    end
    --delete bullet
    if e.distance > 21 or e.distance < -21 then
      del(buls, e)
    end
  end
end
--draw projectilces
function dbullets()
  for e in all(buls) do
    spr(e.sprite, e.x, e.y)
  end
end

